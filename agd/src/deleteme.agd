        SPAWNED
            ;En settina le ponemos que es de tipo 1 boss
            LET SETTINGA = 1
            ;Le ponemos el sprite de la bala
            LET IMAGE = 29
            LET DIRECTION = U
            LET X = V
            LET Y = W
        ENDSPRITE
        ;Almacenamos el contador para que no se vuelva a disparar en ese segundo
        LET L = Z
    ENDIF

