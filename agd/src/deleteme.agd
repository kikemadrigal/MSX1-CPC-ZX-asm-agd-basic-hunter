    ;Si es un bloque que sube y baja
    ELSE
        ;Conservamos la posicin y del bloque
        LET Q = Y
        ;vamos al player
        OTHER
        ; Si el bloque est ms bajo que el player
        IF Q > Y
            ;Miramos la distancia entre el bloque y el player
            SUBTRACT Y FROM Q
