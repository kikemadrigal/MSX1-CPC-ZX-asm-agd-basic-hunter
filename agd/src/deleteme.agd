        ENDIF
    ENDIF
    ;Si es el boss 1
    IF SETTINGA = 1
        IF DIRECTION = UP
            ;Si puede ir a la izquierda lo movemos a la izquierda
            IF CANGOUP
                SPRITEUP
            ;Si no puede ir a la izquierda es que hay un bloque pared
