        IF DIRECTION = LEFT
            SUBTRACT 16 FROM V
            LET U = LEFT
        ; Si no es la derecha puede ser izquierda, arriba o abajo
        ELSE
            IF DIRECTION = RIGHT
                ADD 16 TO V
                LET U = RIGHT
            ELSE
                IF DIRECTION = UP
                    SUBTRACT 20 FROM V
                    LET U = LEFT
                ELSE
                    ADD 20 TO V
                    LET U = RIGHT
                ENDIF
            ENDIF
        ENDIF
        
