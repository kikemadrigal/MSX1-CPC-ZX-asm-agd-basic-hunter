IF KEY LEFT
    LET DIRECTION = LEFT
    LET IMAGE = 1
    IF X <= LEFTEDGE
        LET X = LEFTEDGE
        EXIT
    ELSE
        IF CANGOLEFT
            SPRITELEFT
            ANIMATE
        ENDIF
    ENDIF
ENDIF



IF KEY RIGHT
    LET DIRECTION = RIGHT
    LET IMAGE = 0
    IF X >= RIGHTEDGE
        LET X = RIGHTEDGE
        EXIT
    ELSE
        IF CANGORIGHT
            SPRITERIGHT
            ANIMATE
        ENDIF
    ENDIF
ENDIF


IF KEY UP
    IF LADDERABOVE
        IMAGE=4
        ANIMATE
        SPRITEUP
    ELSE
        JUMP 9
    ENDIF
ENDIF

IF KEY DOWN
    IF LADDERBELOW
        IMAGE=4
        ANIMATE
        SPRITEDOWN
    ENDIF
ENDIF




IF KEY FIRE
    ;Si es contador de número de disparos es menor que 2
    IF D < 2
        ;Con esto hacemos que solo se puedan disparar 5 balas por segundo
        ;Guardamos el segundo en el que estamos en la variable E
        ADD 1 TO D
        LET E = T
        ;Si quedan le quedan balas al player
        IF B>0
            IF P = 0
                SOUND 2
            ELSE
                BEEP 50
            ENDIF
            SPAWN 1 9
            LET U = DIRECTION
            LET V= X
            LET W= Y
            ;Hay que sumarle unos pixeles a la derecha e izquierda a la bala
            IF DIRECTION = LEFT
                SUBTRACT 16 FROM V 
            ELSE
                ADD 16 TO V            
            ENDIF
            ;Con esto configuramos los parámetros de la bala
            SPAWNED
                LET DIRECTION = U
                ; Le decimos que el disparo proviene del player
                LET SETTINGA = 0
                LET X= V
                LET Y= W
            ENDSPRITE
            ;Quitamos 1 al número de balas
            SUBTRACT 1 FROM B
            ;Motramos el número de balas
            AT 0 23
            DISPLAY TRIPLEDIGITS B 
        ;Si no hay balas le quitamos una vida y le recoargamos 100
        ELSE
            KILL
            ADD 100 TO B
            ; Ponemos el mensaje de qu elas balas se agotaron
            AT 9 9          
            MESSAGE 5
